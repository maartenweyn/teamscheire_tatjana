{
  "name": "tatysound",
  "displayName": "TatySound",
  "version": "1.0.0",
  "description": "Companion app for the TatySound sensor",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Maarten Weyn",
  "license": "Apache-2.0",
  "dependencies": {
    "cordova-android": "^8.1.0",
    "cordova-background-timer": "git+https://github.com/dukhanov/cordova-background-timer.git",
    "cordova-browser": "^6.0.0",
    "cordova-plugin-advanced-http": "^2.1.1",
    "cordova-plugin-background-mode": "^0.7.3",
    "cordova-plugin-background-mode-bluetooth-central": "^1.0.0",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-file": "^6.0.2",
    "cordova-sqlite-storage": "3.4.0",
    "onsenui": "^2.10.4"
  },
  "devDependencies": {
    "cordova-ios": "^6.1.1",
    "cordova-plugin-console": "^1.1.0",
    "cordova-plugin-splashscreen": "^6.0.0",
    "cordova-plugin-whitelist": "^1.3.4"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-advanced-http": {
        "OKHTTP_VERSION": "3.10.0"
      },
      "cordova-plugin-device": {},
      "cordova-plugin-background-mode": {},
      "cordova-plugin-background-mode-bluetooth-central": {},
      "cordova-background-timer": {},
      "cordova-sqlite-storage": {},
      "cordova-plugin-ble-central": {
        "BLUETOOTH_USAGE_DESCRIPTION": "This app requires Bluetooth to connect to our hardware."
      },
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-console": {}
    },
    "platforms": [
      "android",
      "browser",
      "ios"
    ]
  }
}