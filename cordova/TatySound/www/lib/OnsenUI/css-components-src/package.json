{
  "name": "",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "gulp serve",
    "build": "npm run build:stylelint && npm run build:postcss && npm run build:minify",
    "clean": "rimraf ./build",
    "build:stylelint": "stylelint 'src/**/*.css'",
    "build:postcss": "postcss src/{*-,}onsen-css-components.css --base src --dir build",
    "build:minify": "cd build && cleancss -o onsen-css-components.min.css onsen-css-components.css && cleancss -o old-onsen-css-components.min.css old-onsen-css-components.css && cleancss -o dark-onsen-css-components.min.css dark-onsen-css-components.css",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "browser-sync": "^2.18.6",
    "eco": "^1.1.0-rc-3",
    "event-stream": "3.3.4",
    "gulp": "^4.0.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-less": "^1.2.3",
    "gulp-load-plugins": "^0.5.0",
    "gulp-order": "^1.0.6",
    "gulp-plumber": "^0.6.1",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-shell": "^0.2.4",
    "gulp-stylelint": "^3.9.0",
    "gulp-stylus": "^1.0.0",
    "gulp-util": "^2.2.14",
    "immutable-css": "^1.1.2",
    "postcss-base64": "^0.6.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-reporter": "^3.0.0",
    "rimraf": "^2.6.1"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "devDependencies": {
    "@onsenui/ancss": "^1.0.0",
    "babel-core": "^6.25.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "clean-css-cli": "^4.3.0",
    "connect-history-api-fallback": "^1.3.0",
    "glob": "^7.1.2",
    "gulp-file": "^0.3.0",
    "js-yaml": "^3.10.0",
    "postcss": "^7.0.32",
    "postcss-apply": "^0.12.0",
    "postcss-calc": "^7.0.3",
    "postcss-cli": "^7.1.1",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.1.1",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.3",
    "postcss-nesting": "^7.0.1",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^0.51.5",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "stylelint": "^13.6.1"
  }
}
